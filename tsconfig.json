{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "types": ["./worker-configuration.d.ts"],
    "lib": ["ESNext"],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "paths": {
      "@/*": ["./src/*"],
      "@core/*": ["./src/modules/core/*"],
      "@auth/*": ["./src/modules/auth/*"],
      "@users/*": ["./src/modules/users/*"]
    }
  }
}
